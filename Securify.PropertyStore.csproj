<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Product>PropertyStore Class Library</Product>
    <Company>Securify B.V.</Company>
    <Authors>Yorick Koster</Authors>
    <Description>.NET Class Library for processing Serialized Property Stores</Description>
    <Copyright>Copyright Â© 2017 Securify B.V.</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageProjectUrl>https://github.com/securifybv/PropertyStore</PackageProjectUrl>
    <RepositoryUrl>https://github.com/securifybv/PropertyStore.git</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageId>Securify.PropertyStore</PackageId>
    <RootNamespace>Securify.PropertyStore</RootNamespace>
    <AssemblyName>Securify.PropertyStore</AssemblyName>
    <Version>1.1.0</Version>
  </PropertyGroup>

</Project>
